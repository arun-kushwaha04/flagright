generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-arm64-openssl-1.1.x", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgres"
  url      = env("DB_CONNECTION_URL")
}


model User {
  id    String  @id
  email String  @unique
  name  String?
}